<extentreports>
    <configuration>
        <theme>DARK</theme>
        <encoding>utf-8</encoding>
        <reportName>AEX Automation Testing Report</reportName>
        <documentTitle>AEX Automation Testing Report</documentTitle>
        <timelineEnabled>true</timelineEnabled>

        <!-- Set logo -->
        <a class="nav-logo">
            <img src="images/AEX-Logo-small.png" style="width: 120px; height: auto;" />
        </a>

        <!-- Custom CSS -->
        <custom-css>
            <![CDATA[
        body {
          background-image: url('images/AEX-Logo-Lockup-White-Small.png');
          background-size: cover;
          background-repeat: no-repeat;
        }
      ]]>
        </custom-css>
    </configuration>
</extentreports>
